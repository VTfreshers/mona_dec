pipeline{
      agent any
        stages {
        stage ('fetch'){
              steps {
                    echo "fetching the source code"
                    }
                 }
        stage ('build'){
              steps {
                    echo "building the source code"
                    }
                 }
        stage('test'){
               steps {
                      echo "testing"
                     }
                  }             
               }
            }  
